{
  "name": "weex-component-mesh",
  "version": "0.2.0",
  "description": "Weex mesh component",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Hanks10100/mesh.git"
  },
  "bugs": {
    "url": "https://github.com/Hanks10100/mesh/issues"
  },
  "scripts": {
    "build:examples": "webpack",
    "build:weex": "node build/build.js weex-component-mesh",
    "build:vue": "node build/build.js vue-mesh",
    "build:react": "node build/build.js react-mesh",
    "dev:examples": "webpack --watch",
    "dev:weex": "node build/build.js weex-component-mesh --watch",
    "dev:vue": "node build/build.js vue-mesh --watch",
    "dev:react": "node build/build.js react-mesh --watch",
    "build": "npm run build:weex && npm run build:vue && npm run build:react && webpack",
    "dev": "npm run dev:weex",
    "test": "mocha --require reify test/*",
    "serve": "serve -p 8080"
  },
  "keywords": [
    "weex",
    "vue",
    "component",
    "mesh"
  ],
  "author": "Hanks <zhanghan.me@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "mocha": "^3.2.0",
    "reify": "^0.4.7",
    "rollup": "^0.41.3",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "serve": "^2.4.0",
    "uglify-js": "^2.8.14",
    "vue": "^2.1.8",
    "vue-loader": "^10.0.2",
    "webpack": "^1.14.0",
    "weex-loader": "^0.4.1",
    "weex-vue-loader": "^0.2.7",
    "weex-vue-render": "^0.1.4"
  }
}
