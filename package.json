{
  "name": "weex-component-mesh",
  "version": "0.1.1",
  "description": "Weex mesh component",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build:examples": "webpack --config build/webpack.examples.config.js",
    "dev:examples": "webpack --watch --config build/webpack.examples.config.js",
    "build": "rollup -c build/rollup.config.js && npm run build:examples",
    "dev": "rollup -w -c build/rollup.config.js",
    "test": "mocha --require reify test",
    "serve": "serve -p 8080"
  },
  "keywords": [
    "weex",
    "vue",
    "component",
    "mesh"
  ],
  "author": "Hanks <zhanghan.me@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "mocha": "^3.2.0",
    "reify": "^0.4.7",
    "rollup": "^0.41.3",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-watch": "^3.2.2",
    "serve": "^2.4.0",
    "vue": "^2.1.8",
    "vue-loader": "^10.0.2",
    "webpack": "^1.14.0",
    "weex-loader": "^0.4.1",
    "weex-vue-loader": "^0.2.7",
    "weex-vue-render": "^0.1.4"
  }
}